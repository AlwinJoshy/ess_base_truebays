    "PSR": {
        "pageFormat": {
            "pageName": "Salary Request",
            "listDisplayUrl": "essapi/CommonApi/",
            "listFieldType": "DateAddressRemarkStrip",
            "recordDataKeys": {
                "titleKey": "Remarks",
                "dateKey": "SubmittedDate",
                "subNoteKey": "ToAddress"
            },
            "addUrl": "essapi/ReqSalaryCertificateApi/",
            "editUrl": "essapi/ReqSalaryCertificateApi/",
            "deleteUrl": "essapi/ReqSalaryCertificateApi/",
            "subPage": {
                "addPageName": "Add Pay Slip Request",
                "editPageName": "Edit Pay Slip Request",
                "dynamicElements": [
                    {
                        "fieldName": "dropDown",
                        "captionText": "Req#",
                        "objectParameterKey": "RequestID",
                        "options": [
                            "111A",
                            "222B",
                            "333C",
                            "444D"
                        ]
                    },
                    {
                        "fieldName": "textArea",
                        "captionText": "Remarks",
                        "objectParameterKey": "Remarks"
                    },
                    {
                        "fieldName": "date",
                        "captionText": "From Date",
                        "objectParameterKey": "SubmittedDate"
                    },
                    {
                        "fieldName": "date",
                        "captionText": "To Date",
                        "objectParameterKey": "SubmittedDate"
                    },
                    {
                        "fieldName": "dropDown",
                        "captionText": "Reason",
                        "objectParameterKey": "Reason",
                        "options": [
                            "Reason - A",
                            "Reason - B",
                            "Reason - B",
                            "Reason - D"
                        ]
                    },
                    {
                        "fieldName": "submitButton",
                        "captionText": "Submit"
                    }
                ]
            }
        }
    },
